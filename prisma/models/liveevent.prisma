model LiveEvent {
  id        String   @id @default(uuid())
  title     String
  subTitle  String
  thumbail  String?
  tags      String[]
  startTime DateTime
  endTime   DateTime
  youtubeLiveUrl String
  // ------------relation----------------
  userId String
  user   User   @relation(fields: [userId], references: [id])



  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
