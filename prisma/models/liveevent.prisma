model LiveEvent {
  id        String   @id @default(uuid())
  title     String
  subTitle  String
  thumbail  String?
  tags      String[]
  startTime DateTime
  endTime   DateTime

  // ------------relation----------------
  userId String
  user   User   @relation(fields: [userId], references: [id])

  // ----------------Agora fields---------------------
  channelName String
  rtcToken    String?
  rtmToken    String?
  uid         String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
